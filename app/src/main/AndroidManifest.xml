<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homemanagement">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddItem"
            android:screenOrientation="portrait" />
        <activity android:name=".ElectricianListItem"
            android:screenOrientation="portrait"/>
        <activity android:name=".PlumberListItem"
            android:screenOrientation="portrait"/>
        <activity android:name=".GardnerListItem"
            android:screenOrientation="portrait"/>
        <activity android:name=".GroceryListItem"
            android:screenOrientation="portrait"/>
        <activity android:name=".MechanicListItem"
            android:screenOrientation="portrait"/>
        <activity android:name=".BabySitterListItem"
            android:screenOrientation="portrait"/>

    </application>

</manifest>